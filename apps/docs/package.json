{
    "name": "docs",
    "description": "The documentation website",
    "version": "0.4.5",
    "private": true,
    "type": "module",
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "start": "next start",
        "dev": "next dev",
        "build": "next build",
        "copy:ai-docs-to-public": "shx rm -rf public/ai-docs && mkdir -p public/ai-docs && shx cp -r dist/ai-docs public",
        "build:ai-docs": "tsx --tsconfig ./scripts/ai-utils/tsconfig.json scripts/buildAiDocs.ts && pnpm copy:ai-docs-to-public",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint --ext js,ts,jsx,tsx,mdx",
        "generate": "pnpm run \"/^generate:.*/\"",
        "generate:componentData": "tsx scripts/generateComponentData.ts",
        "generate:previewRef": "tsx scripts/generatePreviewRef.ts",
        "copy:images": "tsx scripts/copyImages.ts"
    },
    "dependencies": {
        "@tanstack/react-table": "^8.20.5",
        "clsx": "^2.1.1",
        "contentlayer2": "^0.5.8",
        "next": "^14.2.15",
        "next-contentlayer2": "^0.5.8",
        "next-mdx-remote": "^5.0.0",
        "react": "^18.3.1",
        "react-aria": "^3.44.0",
        "react-aria-components": "^1.13.0",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.63.0",
        "react-toggle": "^4.1.3",
        "rehype-parse": "^9.0.1",
        "rehype-pretty-code": "^0.14.1",
        "rehype-react": "^8.0.0",
        "unified": "^11.0.5",
        "unist-util-visit": "^5.0.0"
    },
    "devDependencies": {
        "@codesandbox/sandpack-react": "2.19.10",
        "@eslint/js": "9.38.0",
        "@hopper-ui/components": "workspace:*",
        "@hopper-ui/icons": "workspace:*",
        "@hopper-ui/svg-icons": "workspace:*",
        "@hopper-ui/tokens": "workspace:*",
        "@svgr/webpack": "8.1.0",
        "@types/eslint": "9.6.1",
        "@types/mdast": "4.0.4",
        "@types/node": "24.5.1",
        "@types/react": "18.3.13",
        "@types/react-dom": "18.3.1",
        "@workleap/browserslist-config": "2.1.0",
        "@workleap/eslint-configs": "0.0.7",
        "@workleap/postcss-configs": "1.1.0",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "9.38.0",
        "eslint-config-next": "15.5.4",
        "eslint-plugin-mdx": "3.6.2",
        "glob": "11.0.3",
        "logrocket": "10.1.0",
        "mdast": "3.0.0",
        "mdast-util-mdx": "3.0.0",
        "mdx-bundler": "10.1.1",
        "react-docgen-typescript": "2.2.2",
        "rehype-remark": "10.0.1",
        "remark": "15.0.1",
        "remark-frontmatter": "5.0.0",
        "remark-gfm": "4.0.1",
        "remark-mdx": "3.1.1",
        "remark-parse": "11.0.0",
        "remark-stringify": "11.0.0",
        "shiki": "3.13.0",
        "shx": "0.4.0",
        "tsconfig-paths-webpack-plugin": "4.2.0",
        "typescript": "5.5.4",
        "typescript-eslint": "8.46.2",
        "vfile": "6.0.3",
        "vfile-matter": "5.0.1"
    },
