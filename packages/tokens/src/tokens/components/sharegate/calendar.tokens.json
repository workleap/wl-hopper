{
    "comp-calendar": {
        "cell-border-radius": {
            "$type": "borderRadius",
            "$value": "{shape.circle}"
        },
        "cell-text-color": {
            "$type": "color",
            "$value": "{neutral.text}"
        },
        "cell-background-color-hover": {
            "$type": "color",
            "$value": "{neutral.surface-hover}"
        },
        "cell-text-color-hover": {
            "$type": "color",
